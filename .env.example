# ETL Configuration
# Copy this file to .env and adjust values as needed

# Batch size for database operations (default: 100)
ETL_BATCH_SIZE=100

# Maximum retry attempts for API calls (default: 3)
ETL_MAX_RETRIES=3

# Enable incremental loading (default: true)
ETL_INCREMENTAL=true

# Enable data validation (default: true)
ETL_VALIDATE=true

# API timeout in seconds (default: 30)
ETL_API_TIMEOUT=30

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO